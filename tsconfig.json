{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ES2021",
      "strict": true,
      "noImplicitAny": false,
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  